{
  "extends": [
    "babel",
    "prettier",
    "airbnb",
    "standard",
    "plugin:flowtype/recommended"
  ],
  "globals": {
    "fetch": true,
    "sinon": true,
    "localStorage": true,
    "sessionStorage": true,
    "context": true,
    "describe": false,
    "it": false,
    "beforeAll": false,
    "beforeEach": false,
    "afterEach": false,
    "before": false,
    "after": false,
    "expect": true,
    "fail": true,
    "src": true,
    "sync": true,
    "test": true,
    "root": true,
    "jest": true
  },
  "plugins": [
    "flowtype",
    "jest"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [2, { }],
    "import/named": 2,
    "react/forbid-prop-types": 0,
    "strict": 0,
    "jest/no-disabled-tests": "warn",
    "jest/no-focused-tests": "error",
    "jest/no-identical-title": "error"
  },
  "settings": {
    "flowtype": {
      "onlyFilesWithFlowAnnotation": false
    }
  }
}
